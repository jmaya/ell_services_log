<h1>Student Wizard</h1>
<%= form_tag new_entry_path, :method => :get  do %>
  <% @tags.each do |t| %>
    <h3><%=t.name%></h3>
    <% Student.tagged_with(t).each do |s| %>
      <p>
      <%= check_box_tag 'students[]', s.id %>
      <%=s.number %>
      <%=s.first_name%>
      <%=s.last_name%>
      <p>
    <% end %>
  <% end %>
  <%= submit_tag 'Next Step' %>
<% end %>
